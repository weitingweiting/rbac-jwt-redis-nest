{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "tsconfig.json",
    "watchAssets": true,
    "assets": [
      {
        "include": "**/*.json",
        "outDir": "dist",
        "watchAssets": true
      }
    ]
  },
  "root": "src",
  "entryFile": "main"
}