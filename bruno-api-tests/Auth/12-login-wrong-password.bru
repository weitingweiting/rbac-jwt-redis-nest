meta {
  name: 用户登录 - 密码错误
  type: http
  seq: 12
}

post {
  url: {{authBaseUrl}}/login
  body: json
  auth: none
}

body:json {
  {
    "username": "admin",
    "password": "wrongpassword"
  }
}

assert {
  res.status: eq 401
}

tests {
  test("密码错误验证", function() {
    expect(res.status).to.equal(401);
    expect(res.body.data.message).to.include('用户名或密码错误');
  });
}