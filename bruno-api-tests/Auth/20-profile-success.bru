meta {
  name: 获取用户信息 - 成功
  type: http
  seq: 20
}

get {
  url: {{authBaseUrl}}/profile
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取用户信息成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property('username');
    expect(res.body).to.have.property('email');
  });
}