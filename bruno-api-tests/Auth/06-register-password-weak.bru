meta {
  name: 用户注册 - 密码强度不足
  type: http
  seq: 6
}

post {
  url: {{authBaseUrl}}/register
  body: json
  auth: none
}

body:json {
  {
    "username": "testUser",
    "email": "test@example.com",
    "password": "test123456"
  }
}

assert {
  res.status: eq 400
}

tests {
  test("密码强度验证", function() {
    expect(res.status).to.equal(400);
    expect(res.body.data.message).to.include('密码必须包含大小写字母和数字');
  });
}