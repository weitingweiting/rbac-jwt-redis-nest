meta {
  name: 删除组件 - 组件不存在
  type: http
  seq: 41
}

delete {
  url: {{baseUrl}}/components/NonExistentComponent999
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 404
}

tests {
  test("组件不存在返回404", function() {
    expect(res.status).to.equal(404);
    expect(res.body).to.have.property('message');
  });
}
