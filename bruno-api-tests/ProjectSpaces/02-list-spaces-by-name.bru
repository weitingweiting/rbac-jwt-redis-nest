meta {
  name: 获取项目空间列表 - 按名称查询
  type: http
  seq: 2
}

get {
  url: {{projectSpacesBaseUrl}}?name=test-space-9&page=1&limit=10
  body: none
  auth: bearer
}

params:query {
  name: test-space-9
  page: 1
  limit: 10
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("按名称查询项目空间成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('items');
    expect(res.body.data.items).to.be.an('array');
  });
}
