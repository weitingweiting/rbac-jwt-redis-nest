meta {
  name: 更新项目空间 - 成功
  type: http
  seq: 30
}

put {
  url: {{projectSpacesBaseUrl}}/11
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "description": "更新后的项目空间描述",
    "isOpen": false
  }
}

assert {
  res.status: eq 200
}

tests {
  test("更新项目空间成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('id');
    expect(res.body.data.description).to.equal("更新后的项目空间描述");
    expect(res.body.data.isOpen).to.equal(false);
  });
}
