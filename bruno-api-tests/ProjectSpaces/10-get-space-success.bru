meta {
  name: 获取单个项目空间 - 成功
  type: http
  seq: 10
}

get {
  url: {{projectSpacesBaseUrl}}/{{spaceId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取单个项目空间成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('id');
    expect(res.body.data).to.have.property('name');
    expect(res.body.data).to.have.property('owner');
    expect(res.body.data).to.have.property('users');
    expect(res.body.data).to.have.property('projects');
    expect(res.body.data.users).to.be.an('array');
    expect(res.body.data.projects).to.be.an('array');
  });
}
