meta {
  name: 获取单个权限 - 成功
  type: http
  seq: 10
}

get {
  url: {{permissionsBaseUrl}}/{{permissionId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取单个权限成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('id');
    expect(res.body.data).to.have.property('code');
    expect(res.body.data).to.have.property('name');
  });
}
