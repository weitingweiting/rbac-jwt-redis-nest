meta {
  name: 获取权限列表 - 按name查询
  type: http
  seq: 3
}

get {
  url: {{permissionsBaseUrl}}?name=创建&page=1&limit=10
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("按name查询权限成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('items');
    expect(res.body.data.items).to.be.an('array');
  });
}
