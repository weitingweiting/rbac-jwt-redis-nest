meta {
  name: 创建项目资源 - 成功 (json)
  type: http
  seq: 21
}

post {
  url: {{projectAssetsBaseUrl}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "projectId": 1,
    "url": "https://example.com/assets/data-{{timestamp}}.json",
    "type": "json",
    "size": 5120,
    "meta": {
      "version": "1.0.0"
    }
  }
}

assert {
  res.status: eq 201
}

tests {
  test("创建JSON资源成功", function() {
    expect(res.status).to.equal(201);
    expect(res.body.data).to.have.property('id');
    expect(res.body.data.type).to.equal('json');
  });
}
