meta {
  name: 创建项目资源 - 项目不存在
  type: http
  seq: 23
}

post {
  url: {{projectAssetsBaseUrl}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "projectId": 99999,
    "url": "https://example.com/assets/invalid.jpg",
    "type": "image"
  }
}

assert {
  res.status: eq 404
}

tests {
  test("项目不存在返回404", function() {
    expect(res.status).to.equal(404);
    expect(res.body).to.have.property('message');
  });
}
