meta {
  name: 获取项目资源列表 - 按类型查询
  type: http
  seq: 3
}

get {
  url: {{projectAssetsBaseUrl}}?type=image&page=1&limit=10
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("按类型查询资源成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('items');
    expect(res.body.data.items).to.be.an('array');
  });
}
