meta {
  name: 获取单个项目资源 - 成功
  type: http
  seq: 10
}

get {
  url: {{projectAssetsBaseUrl}}/{{assetId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取单个项目资源成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('id');
    expect(res.body.data).to.have.property('url');
    expect(res.body.data).to.have.property('type');
    expect(res.body.data).to.have.property('project');
  });
}
