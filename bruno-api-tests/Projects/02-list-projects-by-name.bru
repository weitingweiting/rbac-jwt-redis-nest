meta {
  name: 获取项目列表 - 按名称查询
  type: http
  seq: 2
}

get {
  url: {{projectsBaseUrl}}?name=测试&page=1&limit=10
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("按名称查询项目成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body.data).to.have.property('items');
    expect(res.body.data.items).to.be.an('array');
  });
}
