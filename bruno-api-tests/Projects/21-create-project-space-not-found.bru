meta {
  name: 创建项目 - 项目空间不存在
  type: http
  seq: 21
}

post {
  url: {{projectsBaseUrl}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "name": "test-project-invalid",
