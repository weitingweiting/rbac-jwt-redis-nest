meta {
  name: 按状态筛选版本列表
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/components/{{testComponentId}}/versions?status=published&page=1&limit=10
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

tests {
  test("状态码应为 200", function() {
    expect(res.status).to.equal(200);
  });

  test("应返回成功消息", function() {
    const body = res.body;
    expect(body.message).to.equal("获取版本列表成功");
  });

  test("所有版本状态应为 published", function() {
    const body = res.body;
    if (body.data.length > 0) {
      body.data.forEach(version => {
        expect(version.status).to.equal("published");
      });
    }
  });
}
