meta {
  name: 获取单个用户 - 成功
  type: http
  seq: 10
}

get {
  url: {{usersBaseUrl}}/5
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取单个用户成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property('id');
    expect(res.body).to.have.property('username');
  });
}
