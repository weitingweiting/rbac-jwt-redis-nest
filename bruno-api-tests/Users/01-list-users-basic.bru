meta {
  name: 获取用户列表 - 基本分页
  type: http
  seq: 1
}

get {
  url: {{usersBaseUrl}}?page=1&limit=10
  body: none
  auth: bearer
}

params:query {
  page: 1
  limit: 10
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 200
}

tests {
  test("获取用户列表成功", function() {
    expect(res.status).to.equal(200);
    expect(res.body).to.have.property('data');
    expect(res.body).to.have.property('total');
    expect(res.body).to.have.property('page');
    expect(res.body).to.have.property('limit');
  });
}
