meta {
  name: 获取单个用户 - 不存在
  type: http
  seq: 11
}

get {
  url: {{usersBaseUrl}}/9999
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 404
}

tests {
  test("用户不存在验证", function() {
    expect(res.status).to.equal(404);
    expect(res.body.message).to.include('用户不存在');
  });
}