meta {
  name: 强制用户登出
  type: http
  seq: 20
}

post {
  url: {{usersBaseUrl}}/force-logout/4
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

assert {
  res.status: eq 201
}

tests {
  test("强制用户登出成功", function() {
    expect(res.status).to.equal(201);
    expect(res.body).to.have.property('id');
    expect(res.body).to.have.property('username');
  });
}
